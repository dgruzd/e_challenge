= render 'new'

.member_list
  - @members.each do |member|
    .member
      %p
        %h2.member_name
          = link_to member.name, member_path(member)
          %span.friends_count= member.friends_count
        - short_url = shortener_url(member.short_url_key)
        = link_to short_url, short_url, target: '_blank'

= paginate @members